{
  "expo": {
    "name": "apnimandi",
    "slug": "apnimandi",
    "version": "1.0.0",
    "newArchEnabled": true,

    "scheme": "apnimandi",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true,

      "tsconfigPaths": true
    },

    "orientation": "portrait",
    "icon": "./assets/images/ic_launcher.PNG",

    "userInterfaceStyle": "light",

    "splash": {
      "image": "./assets/images/launch_screen.PNG",
      "backgroundColor": "#09b44c"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.PNG",
        "backgroundColor": "#ffffff"
      }
    }
  }
}
